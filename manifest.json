{
	"name": "Open in Habra.",
	"description" : "Extension for open posts from habr.com in habra.vercel.app",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["webRequest", "*://*.habr.com/*", "*://*.geekr.vercel.app/*", "webRequestBlocking", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "icons/habra.png"
	}
}
